[Unit]
Description=stocks jetty 
After=network.target

[Service]
Type=simple
PrivateDevices=yes
SyslogLevel=err
User=stocks
Group=users

WorkingDirectory=/usr/share/stocks/root/jetty-base
ExecStart=/usr/bin/java -jar $(cat ../stocks.properties) /opt/jetty/start.jar
KillSignal=SIGQUIT
KillMode=mixed

[Install]
WantedBy=multi-user.target
